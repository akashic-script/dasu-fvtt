{
  "id": "dasu",
  "title": "Daemon Summoner",
  "description": "The Daemon Summoner system for FoundryVTT!",
  "authors": [
    {
      "name": "Spyrella",
      "discord": "spyrella",
      "url": "https://github.com/spyrella/"
    }
  ],
  "documentTypes": {
    "Actor": {
        "summoner": {
            "htmlFields": ["biography"]
        },
        "daemon": {
            "htmlFields": ["biography"]
        }
    },
    "Item": {
        "feature": {
            "htmlFields": ["description"]
        },
        "gear": {
            "htmlFields": ["description"]
        },
        "spell": {
            "htmlFields": ["description"]
        }
    }
  },
  "flags": {
    "hotReload": {
      "extensions": ["css", "html", "hbs", "json"],
      "paths": ["css", "lang", "templates"]
    }
  },
  "url": "{{URL}}",
  "license": "LICENSE.txt",
  "readme": "README.md",
  "bugs": "{{URL}}/issues",
  "changelog": "CHANGELOG.md",
  "media": [
    {
      "type": "setup",
      "url": "systems/dasu/assets/anvil-impact.png",
      "thumbnail": "systems/dasu/assets/anvil-impact.png"
    }
  ],
  "version": "{{VERSION}}",
  "compatibility": {
    "minimum": "13.339",
    "verified": "13.339"
  },
  "esmodules": ["module/dasu.mjs"],
  "styles": ["css/dasu.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "packs": [],
  "packFolders": [],
  "socket": false,
  "manifest": "{{MANIFEST}}",
  "download": "{{DOWNLOAD}}",
  "background": "systems/dasu/assets/anvil-impact.png",
  "grid": {
    "type": 1,
    "distance": 5,
    "units": "ft",
    "diagonals": 0
  },
  "primaryTokenAttribute": "hp",
  "secondaryTokenAttribute": "wp"
}
