{
  "id": "dasu",
  "title": "Daemon Summoner",
  "description": "The Daemon Summoner system for FoundryVTT!",
  "authors": [
    {
      "name": "Spyrella",
      "discord": "spyrella",
      "url": "https://github.com/spyrella/"
    }
  ],
  "documentTypes": {
    "Actor": {
      "summoner": {
        "htmlFields": ["biography"]
      },
      "daemon": {
        "htmlFields": ["biography"]
      }
    },
    "Item": {
      "ability": {
        "htmlFields": ["description"]
      },
      "item": {
        "htmlFields": ["description"]
      },
      "weapon": {
        "htmlFields": ["description"]
      },
      "tag": {
        "htmlFields": ["description"]
      },
      "tactic": {
        "htmlFields": ["description"]
      },
      "special": {
        "htmlFields": ["description"]
      },
      "scar": {
        "htmlFields": ["description"]
      },
      "schema": {
        "htmlFields": ["description"]
      },
      "feature": {
        "htmlFields": ["description"]
      },
      "class": {
        "htmlFields": ["description"]
      }
    }
  },
  "flags": {
    "hotReload": {
      "extensions": ["css", "html", "hbs", "json"],
      "paths": ["css", "lang", "templates"]
    }
  },
  "url": "{{URL}}",
  "license": "LICENSE.txt",
  "readme": "README.md",
  "bugs": "{{URL}}/issues",
  "changelog": "CHANGELOG.md",
  "media": [
    {
      "type": "setup",
      "url": "systems/dasu/assets/anvil-impact.png",
      "thumbnail": "systems/dasu/assets/anvil-impact.png"
    }
  ],
  "version": "{{VERSION}}",
  "compatibility": {
    "minimum": "13.348",
    "verified": "13.348"
  },
  "esmodules": ["module/dasu.mjs"],
  "styles": ["css/dasu.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "packs": [
    {
      "label": "Handouts",
      "type": "JournalEntry",
      "name": "handouts",
      "path": "packs/handouts",
      "system": "dasu",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "packFolders": [],
  "socket": false,
  "manifest": "{{MANIFEST}}",
  "download": "{{DOWNLOAD}}",
  "background": "systems/dasu/assets/anvil-impact.png",
  "grid": {
    "type": 1,
    "distance": 5,
    "units": "ft",
    "diagonals": 0
  },
  "primaryTokenAttribute": "system.stats.hp",
  "secondaryTokenAttribute": "system.stats.wp"
}
